{
  public static void main(String[] args)
  {
    Scanner input = new Scanner(System.in);
    
    System.out.println("What is your first name: ");
    String name = input.next();
    
    System.out.println("How many credits do you need to graduate: ");
    int creditsToGraduate = input.nextInt();
    
    System.out.println("How many credits have you deleted: ");
    int creditsCompleted = input.nextInt();
    
    System.out.println("How many credits do you take per semester: ");
    int creditsPerSemester = input.nextInt();
    
    
  }
}
